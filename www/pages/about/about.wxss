page{
	font-size: 14px;
	font-family: 'Microsoft YaHei';
	background-color: white;
}

.logo-box{
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	height: 180px;
	background-color: #1abc9c;
	color: white;
}

.logo-box image{
	height: 80px;
	width: 80px;
	border-radius: 100%;
}

.logo-box view{
	margin-top: 10px;
	text-align: center;
}

.about-content{
	padding: 15px;
	color: #1abc9c;
}
